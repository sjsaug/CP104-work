# Imports

# Constants

def main():
    midterm = float(input())
    exam = float(input())
    midterm_weight = 0.35
    exam_weight = 0.65
    final_grade = (midterm * midterm_weight) + (exam * exam_weight)
    print(final_grade)
    
main()